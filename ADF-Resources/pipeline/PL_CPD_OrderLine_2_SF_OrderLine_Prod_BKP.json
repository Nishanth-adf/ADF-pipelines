	"properties": {
		"activities": [
			{
				"name": "PL_CPD_OrderLine_2_SF_OrderLine",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "\nSELECT --TOP 1000\nInvoiceNumber=fois.OrderInvoicesalesId\n,Description__c\t\t\t=\t\tCASE WHEN dp.description IS NULL THEN 'NULL From Source' WHEN dp.description ='' THEN 'Empty from Source' ELSE dp.description END\n,Discount__c\t\t\t=\t\tfois.LineTotalDiscountAmount\n,Order__c\t\t\t\t=\t\tsbo.id\n,Price__c\t\t\t\t=\t\tfois.unitprice\n,Product__c\t\t\t\t=\t\tsbp.id\n,Quantity__c\t\t\t=\t\tfois.quantity\nFROM dbo.factOrderInvoiceSales_D365 fois\nINNER JOIN dbo.dimOrder_D365 do ON do.orderID = fois.orderID\nInner JOIN dbo.sf_ba_Order_Prod sbo ON do.orderNumber=sbo.orderNumber AND fois.invoiceNumber=sbo.invoicenumber\nINNER JOIN dbo.dimproductmaster_D365 dp on fois.productid=dp.productid\nLEFT JOIN dbo.sf_ba_Products_Prod sbp ON dp.catalogNumber=sbp.catalogNumber\nwhere do.OrderId <>'-1'  and do.orderNumber in (select DISTINCT orderNumber from dbo.sf_ba_Order_Prod where invoicenumber is not null) AND fois.acctyr>='2019' AND fois.amountType in ('Product Sales','Other')\n",
						"partitionOption": "None"
					},
					"sink": {
						"type": "SalesforceSink",
						"writeBatchSize": 10000,
						"writeBehavior": "upsert",
						"externalIdFieldName": "Invoice_Number__c",
						"ignoreNullValues": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
