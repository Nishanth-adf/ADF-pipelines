						"type": "AzureSqlSource",
						"sqlReaderQuery": "SELECT \nPurchase_Unit__c\t\t\t\t\n,ProductCode\t\n,Family\t\t\t\n,Name\t\t\t\n,Product_Line__c FROM\n(SELECT\nPurchase_Unit__c\t\t=\tCASE WHEN dpm.description is NULL THEN dpm.catalogNumber + '-' + dpm.source\n\t\t\t\t\t\t\tELSE dpm.catalogNumber + '-' + dpm.source + '-' +\tdpm.description \tEND\n,ProductCode\t\t\t=\tCASE WHEN dpm.catalogNumber is NULL then 'NULL IN SOURCE' ELSE dpm.catalogNumber  END\n,Family\t\t\t\t\t=\tCASE WHEN dpm.productFamily is NULL then 'NULL IN SOURCE' ELSE dpm.productFamily END\n,Name\t\t\t\t\t=\tCASE WHEN dpm.description is null then 'NULL IN SOURCE' WHEN dpm.description='' THEN 'EMPTY IN SOURCE' ELSE dpm.description END\n,Product_Line__c\t\t=\tCASE WHEN dpm.ProductLine is NULL then 'NULL IN SOURCE' ELSE dpm.productLine END\n,RANKs\t\t\t\t\t=\tROW_NUMBER() over(partition by catalogNumber+source  order by catalogNumber+source)\nFROM dbo.dimProductmaster_D365 dpm Where dpm.productID<>'-1' and dpm.catalogNumber <> '.' AND source='NS'\n)x WHERE X.RANKs = 1 ORDER BY X.ProductCode asc\n",
